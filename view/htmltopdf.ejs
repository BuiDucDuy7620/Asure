<table width="50%">

    <tr>
        <th>Name </th>
        <th>Email </th>
        <th>Message </th>
    </tr>


    <tbody>
        <% talkModel.forEach(function(data) { %>
            <tr>
                <td>
                    <%= data.name %>
                </td>
                <td>
                    <%= data.email %>
                </td>
                <td>
                    <%= data.message %>
                </td>
            </tr>
            <% }); %>
    </tbody>

</table>